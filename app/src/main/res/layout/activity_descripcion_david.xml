<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.milena.parejas.DescripcionDavid"
    android:background="@color/material_deep_teal_500">

    <ImageView
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:id="@+id/imageView13"
        android:src="@drawable/img4"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="  David Romero"
        android:id="@+id/textView13"
        android:layout_below="@+id/imageView13"
        android:layout_alignLeft="@+id/imageView13"
        android:layout_alignStart="@+id/imageView13" />

    <RatingBar
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/ratingBar6"
        android:layout_above="@+id/button11"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text=" El es una persona divertida, de caracter fuerte, organizado y responsable; actualmente estudia Ingenieria de Sistemas en la Universidad Antonio NariÃ±o, esta cursando octavo semestre; Mide 1.45 metros, le gusta el futbol y los videojuegos."
        android:id="@+id/textView14"
        android:textSize="13dp"
        android:textAlignment="center"
        android:textColor="@color/abc_background_cache_hint_selector_material_light"
        android:layout_below="@+id/textView13"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="GUARDAR"
        android:id="@+id/button11"
        android:onClick="volver6"
        android:layout_alignParentBottom="true"
        android:layout_alignRight="@+id/textView14"
        android:layout_alignEnd="@+id/textView14" />
</RelativeLayout>
